{
  "compilerOptions": {
    "incremental": true,
    "target": "esnext",
    "module": "ES2020",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true,
    "noEmit": true,
    "baseUrl": "",
    "jsx": "react",
    "paths": {
      "https://cdn.skypack.dev/react": [
        ".purplehaze/typings/react/index.d.ts"
      ],
      "https://cdn.skypack.dev/remarkable": [
        ".purplehaze/typings/remarkable/index.d.ts"
      ],
      "https://cdn.skypack.dev/highlight.js": [
        ".purplehaze/typings/highlight.js/index.d.ts"
      ]
    }
  },
  "include": [
    "./src/**/*.tsx",
    "./src/**/*.jsx",
    "./src/**/*.ts",
    "./src/**/*.js",
    "./src/purplehaze.d.ts"
  ]
}